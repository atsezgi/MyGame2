<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.mygame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartingScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity" />
        <activity android:name=".SecondLevelActivity" />
        <activity android:name=".ThirdLevelActivity" />
        <activity android:name=".FifthLevelActivity" />
        <activity android:name=".FourtActivity" />
        <activity android:name=".SixthLevelActivity" />
        <activity android:name=".SeventhLevelActivity" />
        <activity android:name=".EighthLevelActivity" />
        <activity android:name=".NinethLevelActivity" />
        <activity android:name=".TenthLevelActivity"></activity>
    </application>

</manifest>